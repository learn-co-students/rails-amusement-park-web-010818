<h1>Attractions#edit</h1>
<p>Find me in app/views/attractions/edit.html.erb</p>

<h1>Edit Attraction</h1>

<%= form_for @attraction do |f| %>
<%= f.label "Name" %> </br>
<%= f.text_field :name %> </br>

<%= f.label "Height Requirement"  %> </br>
<%= f.text_field :min_height %> </br>

<%= f.label "How fun is it? (1-5 scale)" %> </br>
<%= f.text_field :happiness_rating %> </br>

<%= f.label "How nausea-inducing is it? (1-5 scale)" %> </br>
<%= f.text_field :nausea_rating %> </br>

<%= f.label "How many tickets does it cost?" %> </br>
<%= f.text_field :tickets %> </br>

<%= f.submit %>

<% end %>
